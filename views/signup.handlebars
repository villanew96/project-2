  <section class="container">
        <div class="columns is-multiline">
          <div class="column is-8 is-offset-2 register">
              
              <div class="field">
                <label class="label">Username</label>
                <div class="control has-icons-left has-icons-right">
                  <input class="input is-success" id="name" type="text" placeholder="Username">
                  <span class="icon is-small is-left">
                    <i class="fas fa-user"></i>
                  </span>
                  <span class="icon is-small is-right">
                    <i class="fas fa-check"></i>
                  </span>
                </div>
                <p class="help is-success hide-error">This username is available</p>
              </div>
              
              <div class="field">
                <label class="label">Password</label>
                <div class="control has-icons-left has-icons-right">
                  <input class="input is-danger" id="password" type="text" placeholder="Password">
                  <span class="icon is-small is-left">
                    <i class="fas fa-lock"></i>
                  </span>
                  <span class="icon is-small is-right">
                    <i class="fas fa-exclamation-triangle"></i>
                  </span>
                </div>
                <p class="help is-danger hide-error">This password is invalid</p>
              </div>

              <div class="field">
                <label class="label">Address</label>
                <div class="control has-icons-left has-icons-right">
                  <input class="input is-danger" id="address" placeholder="Address Input" value="Address">
                  <span class="icon is-small is-left">
                    <i class="fas fa-lock"></i>
                  </span>
                  <span class="icon is-small is-right">
                    <i class="fas fa-exclamation-triangle"></i>
                  </span>
                </div>
                <p class="help is-success">This address is invalid</p>
              </div>
              
              <div class="field">
                <label class="label">Please select one of the following:</label>
                <div class="control">
                  <div class="select" >
                    <select id="type" placeholder="Type" value="Type">
                      <option value="customer">Customer</option>
                      <option value="delivery">Delivery Person</option>
                    </select>
                  </div>
                </div>
              </div>
              
              <div class="field is-grouped">
                <div class="control">
                  <button class="button is-link submit newUser">Submit</button>
                </div>
                <div class="control">
                  <button class="button is-link is-light cancel">Cancel</button>
                </div>
              </div>
          </div>
          <div class="column is-8 is-offset-2">
            <br>
            <nav class="level">
              <div class="level-left">
                <div class="level-item">
                  <span class="icon">
                    <i class="fab fa-github"></i>
                  </span> &emsp;
                </div>
              </div>
              <div class="level-right">
                <small class="level-item" style="color: var(--textLight)">
                  &copy; Super Cool Website. All Rights Reserved.
                </small>
              </div>
            </nav>
          </div>
        </div>
      </section>
      <script src="https://code.jquery.com/jquery.js"></script>
      <script src="./Assets/js/index.js"></script>